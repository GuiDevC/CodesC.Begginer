#include <stdio.h>

int main(void){
    FILE *pFile = fopen("(Place of txt)//(NameOfTxt)", "r");
    char buffer[1024] = {0};

    if(pFile == NULL){
        printf("Connecting is not sucessfull\n");
        return 1;
    }

    while(fgets(buffer, sizeof(buffer), pFile) != NULL){
        printf("%s", buffer);
    }

    fclose(pFile);
    return 0;
}
